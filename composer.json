{
    "name": "chipslays/porter",
    "type": "library",
    "keywords": ["websocket", "realtime", "sockets"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "workerman/workerman": "^4.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0",
        "pestphp/pest": "^1.21 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Porter\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
