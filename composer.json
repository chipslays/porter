{
    "name": "chipslays/porter",
    "type": "library",
    "keywords": ["websocket", "realtime", "chat", "sockets"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "workerman/workerman": "^4.0",
        "chipslays/sauce": "^1.0",
        "chipslays/collection": "^1.1",
        "respect/validation": "^2.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0",
        "pestphp/pest": "^1.21"
    },
    "bin": ["bin/porter"],
    "autoload": {
        "psr-4": {
            "Porter\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
